{
   "name": "WhatsApp Send message to all (Beta)",
   "description" : "Send a semi personalized message to all contacts.",
   "version": "0.1",
   "manifest_version": 2,
   "permissions": [
     "storage",
     "https://web.whatsapp.com/"
   ],

   "background": {
     "scripts": [
       "openWaWeb.js"
     ],
     "persistent": false
   },

   "browser_action": {
     "default_icon": {
       "32": "whatsapp.png"
     }
   }
 }
